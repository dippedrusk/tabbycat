{
  "name": "Tabby Cat",
  "version": "1.0",
  "description": "Shows tab characters in all windows when turned on",
  "permissions": [
    "http://*/*", "https://*/*",
    "tabs","storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Tabby Cat"
  },
  "manifest_version": 2
}
